<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_gravity="center"
    android:layout_height="10mm"
    tools:ignore="InOrMmUsage">


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/stl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/mtl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.08"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide_left"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.2"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide_right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.6"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/str1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.65"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/mtr1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.68"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/mtr2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.92"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/str2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95"
        />



    <View
        android:id="@+id/indicator_neg2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintTop_toTopOf="parent"/>

    <View
        android:id="@+id/indicator_neg1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintTop_toBottomOf="@id/indicator_neg2"
        app:layout_constraintBottom_toTopOf="@id/indicator_0"/>


    <View
        android:id="@+id/indicator_1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintTop_toBottomOf="@id/indicator_0"
        app:layout_constraintBottom_toTopOf="@id/indicator_2"
        />

    <View
        android:id="@+id/indicator_2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintTop_toBottomOf="@id/indicator_1"
        app:layout_constraintBottom_toTopOf="@id/indicator_3"
        />

    <View
        android:id="@+id/indicator_3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintTop_toBottomOf="@id/indicator_2"
        app:layout_constraintBottom_toTopOf="@id/indicator_4"
        />

    <View
        android:id="@+id/indicator_4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintTop_toBottomOf="@id/indicator_3"
        app:layout_constraintBottom_toTopOf="@id/indicator_5"
        />



    <View
        android:id="@+id/indicator_6"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintTop_toBottomOf="@id/indicator_5"
        app:layout_constraintBottom_toTopOf="@id/indicator_7"
        />

    <View
        android:id="@+id/indicator_7"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintTop_toBottomOf="@id/indicator_6"
        app:layout_constraintBottom_toBottomOf="parent"
        />


    <View
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="@color/white"
        app:layout_constraintStart_toStartOf="@id/stl"
        app:layout_constraintEnd_toEndOf="@+id/guide_right" />

    <View
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:alpha="0.95"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="@id/str2"
        app:layout_constraintStart_toEndOf="@id/str1" />


    <View
        android:id="@+id/indicator_5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintTop_toBottomOf="@id/indicator_4"
        app:layout_constraintBottom_toTopOf="@id/indicator_6"
        />

    <View
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:background="@color/white"
        app:layout_constraintStart_toStartOf="@id/mtl"
        app:layout_constraintEnd_toEndOf="@+id/guide_right"
        app:layout_constraintTop_toTopOf="@id/indicator_5"
        app:layout_constraintBottom_toBottomOf="@id/indicator_5"
        />

    <View
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:background="@color/white"
        android:alpha="0.9"
        app:layout_constraintEnd_toEndOf="@id/mtr2"
        app:layout_constraintStart_toEndOf="@id/mtr1"
        app:layout_constraintTop_toTopOf="@id/indicator_5"
        app:layout_constraintBottom_toBottomOf="@id/indicator_5" />

    <View
        android:id="@+id/indicator_0"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/fgBlue"
        app:layout_constraintTop_toBottomOf="@id/indicator_neg1"
        app:layout_constraintBottom_toTopOf="@id/indicator_1"
        />

    <View
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:background="@color/white"
        android:layout_marginStart="-4dp"
        app:layout_constraintStart_toStartOf="@id/text_left"
        app:layout_constraintEnd_toEndOf="@+id/guide_right"
        app:layout_constraintTop_toTopOf="@id/indicator_0"
        app:layout_constraintBottom_toBottomOf="@id/indicator_0"
        />

    <TextView
        android:id="@+id/text_left"
        android:layout_width="24sp"
        android:layout_height="24sp"
        android:gravity="center"
        android:rotation="90"
        android:text="10"
        android:textColor="@color/fgBlue"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/indicator_0"
        app:layout_constraintEnd_toStartOf="@+id/guide_left"
        app:layout_constraintStart_toStartOf="@id/mtl"
        app:layout_constraintTop_toTopOf="@id/indicator_0"
        tools:ignore="HardcodedText" />





    <TextView
        android:id="@+id/text_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:gravity="center"
        android:paddingHorizontal="4dp"
        android:text="10 cm"
        android:textColor="@color/fgBlue"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/indicator_0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/guide_right"
        app:layout_constraintTop_toTopOf="@id/indicator_0"
        tools:ignore="HardcodedText" />


    <View
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="@color/fgBlue"
        app:layout_constraintEnd_toEndOf="@id/guide_right"
        app:layout_constraintStart_toEndOf="@id/guide_left"
        app:layout_constraintTop_toTopOf="parent" />




</androidx.constraintlayout.widget.ConstraintLayout>